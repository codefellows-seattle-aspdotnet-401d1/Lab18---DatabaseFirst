@model IEnumerable<lab18_brian.Models.PackingListItems>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PackingList)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Item)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PackingList.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Item.Name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.packinglistID }) |
                @Html.ActionLink("Details", "Details", new { id=item.packinglistID }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.packinglistID })
            </td>
        </tr>
}
    </tbody>
</table>
