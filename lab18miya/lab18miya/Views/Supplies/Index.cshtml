@model IEnumerable<lab18miya.Models.Supplies>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Quantity)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Weather)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quantity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Weather.WeatherID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SupplyID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SupplyID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SupplyID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
